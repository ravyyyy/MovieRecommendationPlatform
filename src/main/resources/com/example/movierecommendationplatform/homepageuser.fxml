<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600" prefWidth="920" style="-fx-background-color: #2D3447;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.movierecommendationplatform.HomepageuserController">
   <children>
      <TableView fx:id="tableMovie" layoutX="123.0" layoutY="111.0" prefHeight="367.0" prefWidth="659.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="37.0" text="#" />
          <TableColumn fx:id="name" prefWidth="318.0" text="Name" />
            <TableColumn fx:id="category" minWidth="0.0" prefWidth="95.0" text="Category" />
            <TableColumn fx:id="releaseDate" minWidth="0.0" prefWidth="138.0" text="Release Date" />
            <TableColumn fx:id="rating" minWidth="0.0" prefWidth="69.0" text="Rating" />
        </columns>
      </TableView>
      <Button fx:id="likeButton" layoutX="812.0" layoutY="111.0" mnemonicParsing="false" onAction="#likeMovie" prefHeight="13.0" prefWidth="84.0" text="LIKE" />
      <Button fx:id="orderByDateButton" layoutX="799.0" layoutY="162.0" mnemonicParsing="false" onAction="#orderByDate" prefHeight="25.0" prefWidth="108.0" text="ORDER BY DATE" />
      <Button fx:id="orderByRatingButton" layoutX="793.0" layoutY="212.0" mnemonicParsing="false" onAction="#orderByRating" prefHeight="25.0" prefWidth="122.0" text="ORDER BY RATING" />
      <Button fx:id="logOutButton" layoutX="392.0" layoutY="520.0" mnemonicParsing="false" onAction="#logOut" prefHeight="25.0" prefWidth="122.0" text="LOG OUT" />
   </children>
</AnchorPane>
